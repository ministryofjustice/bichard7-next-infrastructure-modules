{
  "rules": [
    {
      "rulePriority": 1,
      "description": "Keep 1 untagged image",
      "selection": {
        "tagStatus": "untagged",
        "countType": "imageCountMoreThan",
        "countNumber": 1
      },
      "action": {
        "type": "expire"
      }
    },
    {
      "rulePriority": 10,
      "description": "Expire images older than 21 days",
      "selection": {
        "tagStatus": "any",
        "countType": "sinceImagePushed",
        "countUnit": "days",
        "countNumber": 21
      },
      "action": {
        "type": "expire"
      }
    }
  ]
}
